            <li><a class="navbar-brand"  ng-show="isStudent()" href="#/documents/getFor/{{ userId }}">Documents</a></li>
            <li><a class="navbar-brand"  ng-show="isStudent()" href="#/exams/getFor/{{ userId }}">Exams</a></li>
            <li><a class="navbar-brand"  ng-show="isAdmin()" href="#/exams">Exams</a></li>
