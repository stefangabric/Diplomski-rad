        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a  id="1" class="navbar-brand"  href="#"><img id="logo" alt="eObrazovanje"></a>

    </div>

    <div id="navbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav">

            <li><a class="navbar-brand"  ng-show="isAdmin()"  href="#/professors">Professors</a>   </li>
            <li><a class="navbar-brand"  ng-show="isAdmin()" href="#/professorRoles">Professor roles</a> </li>
            <li><a class="navbar-brand"  ng-show="isAdmin()" href="#/students">Students</a>   </li>
            <li><a class="navbar-brand"  ng-show="isAdmin()" href="#/subjects">Subjects</a> </li>
            <li><a class="navbar-brand"  ng-show="isStudent()" href="#/subjects/getForS/{{ userId }}" >Subjects</a> </li>
            <li><a class="navbar-brand"  ng-show="isProfessor()" href="#/subjects/getForP/{{ userId }}" >Subjects</a> </li>
            <li><a class="navbar-brand"  ng-show="isAdmin()" href="#/obligations">Obligations</a>    </li>
            <li><a class="navbar-brand"  ng-show="isAdmin()" href="#/transactions">Transactions</a>   </li>
